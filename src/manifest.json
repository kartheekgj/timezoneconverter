{
    "manifest_version": 3,
    "name": "Time Zone Converter",
    "version": "1.0",
    "description": "Convert IST to CET and US time zones",
    "action": {
      "default_popup": "pop.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["storage"]
  }
  